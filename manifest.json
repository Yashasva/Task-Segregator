{
    "name": "Tab Segregator",
    "description": "This segregates tabs according to project",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["storage", "tabs"],

    "action": {
        "default_popup": "popup.html"
    }
}